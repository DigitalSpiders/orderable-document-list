{
  "name": "@sanity/orderable-document-list",
  "version": "1.0.0-v3-studio.4",
  "description": "Drag-and-drop Document Ordering without leaving the Editing surface",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "default": "./lib/index.modern.js"
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.modern.js",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && plugin-kit verify-package --silent && tsc --noEmit",
    "build": "parcel build --no-cache",
    "watch": "parcel watch",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "compile": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier src -w",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sanity-plugin-orderable-document-list"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@sanity/icons": "^1.2.1",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.0.0-beta.31",
    "lexorank": "^1.0.4",
    "prop-types": "^15.8.1",
    "react-beautiful-dnd": "^13.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@parcel/packager-ts": "^2.6.0",
    "@parcel/transformer-typescript-types": "^2.6.0",
    "@sanity/plugin-kit": "^1.1.0-ecosystem-preset.5",
    "@sanity/semantic-release-preset": "^2.0.1",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "autoprefixer": "^10.3.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^5.1.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "parcel": "^2.6.0",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "3.0.0-dev-preview.22",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "sanity": "dev-preview | 3.0.0-dev-preview.22"
  },
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-plugin-orderable-document-list/issues"
  },
  "homepage": "https://github.com/sanity-io/sanity-plugin-orderable-document-list#readme",
  "sanityExchangeUrl": "https://www.sanity.io/plugins/orderable-document-list",
  "engines": {
    "node": ">=14.0.0"
  }
}
